{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": "3.35",
        "targets": {
          "node": "5"
        },
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": ["@babel/plugin-syntax-top-level-await", "babel-plugin-transform-import-meta"]
}
