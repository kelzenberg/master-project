{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Slider Output",
  "description": "JSON Schema for the slider output send back to the simulation",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "value": {
      "type": "number"
    }
  },
  "required": ["name", "value"],
  "additionalProperties": "false"
}
