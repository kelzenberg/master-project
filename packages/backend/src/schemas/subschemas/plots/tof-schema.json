{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "TOF",
  "description": "JSON Schema for the tof plot in initial data",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "label": {
        "type": "string"
      },
      "color": {
        "type": "array",
        "items": {
          "type": "number"
        },
        "minItems": 3,
        "maxItems": 3
      }
    },
    "required": ["label", "color"]
  }
}
