{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Species",
  "description": "JSON Schema for the species in initial data",
  "type": "array",
  "items": {
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        },
        "z": {
          "type": "number"
        },
        "type": {
          "type": "string"
        }
      },
      "required": ["x", "y", "z", "type"]
    }
  }
}
